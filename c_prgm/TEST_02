
01.
#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int ans=0;
    
    	if(n>=1 &&n<=50){
    		ans=30;
		}
		else if(n>=51 && n<=150){
			ans=40;
		}
		else{
			ans=50;
		}
	
	printf("Electciy bill is: %d ",ans);
    return 0;
}
_______________________________________
02.+/-/0
_______________________________________
#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    
	if(n<0){
		printf("Negative");
	} 
	else if(n>0){
		printf("Positive");
	}
	else{
		printf("NEutral");
	}

    return 0;
}
